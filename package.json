{
  "name": "@developbharat/youtubebot",
  "version": "2.0.1",
  "description": "A Robot to use Youtube Services",
  "main": "lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/*",
    "LICENCE",
    "README",
    "package.json"
  ],
  "scripts": {
    "test": "tap run --disable-coverage --allow-empty-coverage",
    "build": "rm -rf lib && tsc",
    "format": "prettier --write src"
  },
  "keywords": [],
  "author": {
    "email": "team@developbharat.com",
    "name": "Develop Bharat",
    "url": "https://developbharat.com"
  },
  "readme": "README.md",
  "repository": {
    "directory": "/",
    "type": "https",
    "url": "https://github.com/developbharat/YoutubeBot"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 120,
    "quoteProps": "consistent",
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "singleQuote": true,
    "semi": true,
    "proseWrap": "always"
  },
  "license": "GPLv3",
  "bugs": {
    "email": "team@developbharat.com",
    "url": "https://github.com/developbharat/YoutubeBot/issues"
  },
  "engines": {
    "node": ">=14.x"
  },
  "homepage": "https://github.com/developbharat/YoutubeBot",
  "maintainers": [
    {
      "email": "work.jayantmalik@gmail.com",
      "name": "Jayant Malik",
      "url": "https://developbharat.com"
    }
  ],
  "devDependencies": {
    "prettier": "^3.2.4",
    "tap": "^18.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "miniget": "^4.2.3",
    "zod": "^3.22.4"
  }
}
